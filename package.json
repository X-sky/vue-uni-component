{
  "name": "vue-uni-component",
  "version": "0.0.1-beta.0",
  "private": true,
  "description": "",
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "dev:2": "pnpm -F ./containers/v2 dev",
    "dev:27": "pnpm -F ./containers/v2.7 dev",
    "dev:3": "pnpm -F ./containers/v3 dev",
    "dev:cdn": "pnpm -F ./cdn-playground dev",
    "build": "esno ./scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "release": "bumpp",
    "create:ui": "esno ./scripts/cli/createComp.ts",
    "clean": "rimraf --glob ./**/node_modules ./**/dist dist node_modules"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.197",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.5",
    "@types/prompts": "^2.4.4",
    "bumpp": "^9.1.1",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.4.2",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "lodash-es": "^4.17.21",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "sass": "^1.65.1",
    "typescript": "^5.1.6",
    "vite": "~4.4.7",
    "vue": "~3.2.47"
  },
  "dependencies": {
    "vue-demi": "^0.14.5"
  },
  "pnpm": {
    "packageExtensions": {
      "vue-template-compiler": {
        "peerDependencies": {
          "vue": "~2.6.14"
        }
      }
    },
    "overrides": {
      "vue-demi": "$vue-demi",
      "vite": "$vite"
    }
  }
}
